#Problem 1
class Person: 
  def __init__(name,age):
    self.name = name
    self.age = age 
    self.greet = greet

  def display_info(self):
    print(f"Name: {self.name}, Age: {self.age}, Greet: {self.greet}")

Person1 = Person("Pete","28","Hello, I am Pete! Nice to meet you!")

