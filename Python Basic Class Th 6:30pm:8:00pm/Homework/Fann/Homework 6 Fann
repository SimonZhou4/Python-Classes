2.
name = str(input("Enter your first and last name:"))
start = "Hello, my name is "
namestart = start + name
print(namestart)

3.
name = str(input("Enter your first and last name:"))
start = "Hello, my name is "
namestart = start + name
print(namestart)
cut_name = name.split()
first = cut_name[0]
last = cut_name[1]
print(first)
print(last)

