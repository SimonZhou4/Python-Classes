print(count)

6.
sentance = input("Enter a simple sentance introducing your favorite color:")
upcase = sentance.upper()
lowcase = sentance.lower()
capital = sentance.capitalize()
occur = sentance.count("y")
occur2 = sentance.count("Y")
question = sentance.count("?")
if question == True:
  print("question")
print(upcase)
print(lowcase)
print(capital)
print(occur)
print(occur2)

7.
from datetime import datetime
current = datetime.now()
date = current.strftime("%m:%d:%Y")
print("The current date is,", date)

8.
from datetime import datetime
current = datetime.now()
date = current.strftime("%m:%d:%Y")
month = current.strftime("%m")
day = current.strftime("%d")
year = current.strftime("%Y")
print("The date is", date, ", also known as the", day, "day in the", month, "of the year", year)

