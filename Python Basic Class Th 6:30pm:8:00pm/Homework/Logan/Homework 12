#7
import matplotlib.pyplot as plt
import numpy as np

plt.style.use('_mpl-gallery')

# make data
x = np.linspace(0, 9, 99)
y = 4 + 2 * np.sin(2 * x)

# plot
fig, ax = plt.subplots()

ax.plot(x, y, linewidth=2.0)

ax.set(xlim=(0, 7), xticks=np.arange(1, 8),
       ylim=(0, 7), yticks=np.arange(1, 8))

plt.show()














    






































































































































































































































































































































