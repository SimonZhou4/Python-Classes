import datetime
o = datetime.datetime.now()

print(o)
  


#2
import random

def pick_random_number():
    numbers = list(range(1, 11))
    return random.choice(numbers)

random_number = pick_random_number()
print("Random number picked:", random_number)


#3
import math

def main():
    w = float(input("Enter a number: "))

    square_root = math.sqrt(w)

    print("The square root of", w, "is:",  square_root)

if __name__ == "__main__":
    main()


