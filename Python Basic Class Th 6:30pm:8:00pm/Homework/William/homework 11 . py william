
def apply_oper(x,y):
  vi= int(input("pick one"))
  total=0
  if vi==1:
    total= x*y
  elif vi==2:
    total= x+y
  elif vi== 3:
    total=x-y
  else:
    print("no option for that")
    print("system crash")
  return print(total)

num= int(input("pick number"))
num2= int(input("another one"))
print("1.Multiplcation")
print("2.addition")
print("3.subtraction")
apply_oper(num,num2)
