set1={1,2,3,4,5,3,2,2,44,335,22,5,542,22,5,1342,2,}
set2={1,2,3,32,34,21,4,3241,43,53265,342342,224,221,22,3322,121}


n=set1.issubset(set2)
w=set1.union(set2)
d=set1.intersection(set2)
a=set1.difference(set2)
print(n)
print(w)
print(d)
print(a)


