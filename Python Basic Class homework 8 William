
info={
"age":23,
"name":"Bob",
"city":"Chino Hills",
"occupation":"teacher"
}
print(info)
print(info["age"])
print(info["name"])
print(info["city"])
print(info["occupation"])
info["age"]=21
print(info["age"])

inventory={"item1":"slime ball",
"item2":"wolf's claw",
"item3":"sword"}

inventory["item4"]="shield"
del inventory["item1"]
inventory["item2"]="armor"
print(inventory["item2"])
print(inventory)

